<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="daqianjietong.com.daqianjietong">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- if you want to load images from a file OR from the internet -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>

    <application
        android:name="daqianjietong.com.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="daqianjietong.com.GuideActivity"
                  android:theme="@style/AppTheme.NoActionBar"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        //登录
        <activity android:name="daqianjietong.com.activity.UserLoginActivity"
                  android:theme="@style/AppTheme.NoActionBar"/>
        //注册
        <activity android:name="daqianjietong.com.activity.RegisterUserActivity"
                  android:theme="@style/AppTheme.NoActionBar"/>
        //忘记密码
        <activity android:name="daqianjietong.com.activity.FrogetPsdActivity"
                  android:theme="@style/AppTheme.NoActionBar"/>
        //主页
        <activity android:name="daqianjietong.com.activity.MainActivity"
            android:screenOrientation="portrait"
                  android:theme="@style/AppTheme.NoActionBar"/>
        //个人中心
        <activity android:name="daqianjietong.com.activity.PersonalActivity"
                  android:theme="@style/AppTheme.NoActionBar"/>
        //修改密码
        <activity android:name="daqianjietong.com.activity.RestorePsdActivity"
                  android:theme="@style/AppTheme.NoActionBar"/>
        //个人信息
        <activity android:name="daqianjietong.com.activity.PersonalInfoActivity"
                  android:theme="@style/AppTheme.NoActionBar"/>
        //预约车位
        <activity android:name="daqianjietong.com.activity.OrderCarParkActivity"
                  android:theme="@style/AppTheme.NoActionBar"/>
        //租车位
        <activity android:name="daqianjietong.com.activity.RentCarParkActivity"
                  android:theme="@style/AppTheme.NoActionBar"/>
        //附近车位
        <activity android:name="daqianjietong.com.activity.NearCarParkActivity"
                  android:theme="@style/AppTheme.NoActionBar"/>
        //停车券
        <activity android:name="daqianjietong.com.activity.ParkCardActivity"
                  android:theme="@style/AppTheme.NoActionBar"/>
        //我的订单
        <activity android:name="daqianjietong.com.activity.MyOrderActivity"
                  android:theme="@style/AppTheme.NoActionBar"/>

    </application>

</manifest>